{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "my-instants-dc-nodejs",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "rm -rf dist && tsc",
		"lint": "prettier --check . && eslint --ext .ts --format=pretty src",
		"deploy": "node --env-file=.env dist/util/deploy.js",
		"delete-commands": "node --env-file=.env dist/util/delete.js",
		"list-commands": "node --env-file=.env dist/util/listCommands.js",
		"format": "prettier --write . && eslint --ext .ts --fix --format=pretty src",
		"start": "node --env-file=.env dist/index.js",
		"dev": "nodemon"
	},
	"dependencies": {
		"@discordjs/core": "^2.3.0",
		"@discordjs/voice": "^0.19.0",
		"@snazzah/davey": "^0.1.8",
		"axios": "^1.13.2",
		"cheerio": "^1.1.2",
		"discord.js": "^14.24.0",
		"ffmpeg-static": "^5.3.0",
		"opusscript": "^0.0.8",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@sapphire/ts-config": "^5.0.1",
		"@types/cheerio": "^0.22.35",
		"@types/node": "^22.18.8",
		"eslint": "^9.37.0",
		"eslint-config-neon": "^0.2.7",
		"eslint-formatter-compact": "^8.40.0",
		"eslint-formatter-pretty": "^7.0.0",
		"nodemon": "^3.1.7",
		"prettier": "^3.6.2",
		"typescript": "~5.9.3"
	},
	"engines": {
		"node": ">=22.12.0"
	}
}
